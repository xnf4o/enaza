{
    "name": "xnf4o/enaza",
    "description": "Laravel Enaza package",
    "license": "license",
    "authors": [
        {
            "name": "xnf4o",
            "email": "xnf4o@inbox.ru"
        }
    ],
    "homepage": "https://github.com/xnf4o/enaza",
    "keywords": ["Laravel", "enaza"],
    "require": {
        "illuminate/support": "~5|~6|~7",
        "guzzlehttp/guzzle": "^6.5",
        "midnite81/xml2array": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3|~4",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "xnf4o\\enaza\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "xnf4o\\enaza\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "xnf4o\\enaza\\enazaServiceProvider"
            ],
            "aliases": {
                "enaza": "xnf4o\\enaza\\Facades\\enaza"
            }
        }
    }
}
